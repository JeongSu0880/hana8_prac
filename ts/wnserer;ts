type Reading = { page: number }
interface IReading {
    page: number;
}
type Writing = { title: string }

type ReadWrite = Reading | Writing;
type ReadAndWrite = Reading & Writing;
//유니온 -> 교집합, 인터섹션 => 합집합ㄴ
interface IReadWrite extends Reading {
    title: string;
}

const x1: ReadWrite = { title: 'XXXXX' }
const x2: ReadWrite = { title: 'XXXX', page: 33 }
// 클래스도 의미가 이씯 

const yy: IReading = { page: 1 }
const x3: IReadWrite = { title: 'XXXX', page: 33 }

